    },
    "syncSettings.resources": [
        "extensions",
        "keybindings",
        "settings",
        "snippets",
        "uiState"
    ],
    "tabnine.experimentalAutoImports": true,
    "workbench.colorTheme": "Quiet Light",
    "peacock.elementAdjustments": {

        "activityBar": "darken",
        "statusBar": "none",
        "titleBar": "none"
    },
    "peacock.affectTabActiveBorder": true,
    "peacock.affectStatusAndTitleBorders": true,
    "peacock.affectSideBarBorder": true,
    "peacock.affectPanelBorder": true,
    "peacock.affectEditorGroupBorder": true,
    "peacock.affectDebuggingStatusBar": true,
    "workbench.iconTheme": "material-icon-theme",
    "vscode-edge-devtools.webhintInstallNotification": true,
    "[python]": {

        "diffEditor.ignoreTrimWhitespace": false,
        "gitlens.codeLens.symbolScopes": [
            "!Module"
        ],
        "editor.formatOnType": true,
        "editor.wordBasedSuggestions": false,
        "editor.mouseWheelZoom": true,
        "editor.defaultFormatter": "ms-python.python"
    },
    "sonarlint.rules": {
        "Web:S5256": {
            "level": "off"
        }
    },
    "[cpp]": {
    
        "editor.wordBasedSuggestions": false,
        "editor.suggest.insertMode": "replace",
        "editor.semanticHighlighting.enabled": true,
        "editor.mouseWheelZoom": true
    },
    "win-ca.inject": "append",
    "liveServer.settings.donotShowInfoMsg": true
}